package Exercicio2;

public class OperacoesMatematicas {

	public int somar(int numUm, numDois) {
		soma = numUm + numDois;
		return soma;
	}
	
	public double subtrair(double numUm, numDois) {
		
		return numUm;
	}
	
	public double multiplicar(double numUm, numDois) {
		
		return numUm;
	}
	
	public double dividir(double numUm, numDois) {
		
		return numUm;
	}

}
